{
  "defaultSeverity": "error",
  "extends": ["tslint-config-airbnb", "tslint-react", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "variable-name": false,
    "function-name": false,
    "jsx-no-lambda": false,
    "no-increment-decrement": false,
    "import-name": false,
    "strict-boolean-expressions": false,
    "space-in-brackets": false
  },
  "rulesDirectory": []
}
